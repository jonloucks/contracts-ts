{
  "name": "@jonloucks/contracts-ts",
  "version": "0.2.0",
  "description": "Typescript Dependency Contracts for dependency inversion",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch --watchAll --verbose",
    "test:coverage": "jest --coverage",
    "test:diagnostics": "jest --detectOpenHandles",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "docs": "typedoc --options typedoc.json",
    "badges": "npx tsx ./src/utils/generate-badges.ts"
  },
  "keywords": [
    "typescript",
    "contracts",
    "dependency-injection",
    "dependency-inversion"
  ],
  "author": "Jon Loucks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonloucks/contracts-ts.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  }
}
